{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "env": {
    "VITE_APPWRITE_ENDPOINT": "@vite_appwrite_endpoint",
    "VITE_APPWRITE_PROJECT_ID": "@vite_appwrite_project_id",
    "VITE_APPWRITE_DATABASE_ID": "@vite_appwrite_database_id",
    "VITE_APPWRITE_FUNCTION_PROCESS_REQUEST": "@vite_appwrite_function_process_request",
    "VITE_JWT_SECRET": "@vite_jwt_secret",
    "VITE_APP_URL_AUTH": "@vite_app_url_auth",
    "VITE_APP_URL_PLAYER": "@vite_app_url_player"
  },
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/admin/:path*"
    }
  ]
}
